  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ObjectVersionWQMeatDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@username">
            <Value>=Parameters!username.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT CONVERT(date,[curdate]) [Дата]
 , COUNT([refuuid]) [Всего обращений к документу]
 , COUNT(DISTINCT [refuuid]) [Изменено документов]
 , 100*COUNT(DISTINCT refuuid)/COUNT(refuuid) as [КПД %]
FROM [DWHSchema].[VersionsCompress2]
WHERE  [username] = @username
AND [curdate] &gt; DATEADD(day,-30,GETDATE())
GROUP BY  CONVERT(date,[curdate])
ORDER BY CONVERT(date,[curdate]) DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
